{
  "name": "@khriztianmoreno/storybook-addon-raw-styled-component",
  "version": "0.8.1",
  "description": "A Storybook addon that extracts and displays compiled syntax-highlighted HTML & CSS",
  "main": "index.js",
  "scripts": {
    "build:watch": "babel --watch -d ./lib ./src",
    "build": "npm run clean && babel -d ./lib ./src",
    "clean": "rm -rf ./lib",
    "prepare": "npm run build",
    "prettier": "prettier --write '**/*.{js,json,md,yml}'"
  },
  "keywords": [
    "addon",
    "storybook",
    "styled-components",
    "react",
    "html",
    "css"
  ],
  "author": "khriztianmoreno <khriztianmoreno@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.26.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@storybook/react": "8.4.7",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.23.1",
    "react": "17.0.2"
  },
  "dependencies": {
    "@babel/generator": "^7.8.3",
    "@babel/parser": "^7.8.3",
    "@storybook/addons": "^7.0.0",
    "@storybook/api": "^7.0.0",
    "@storybook/components": "^8.0.0",
    "estraverse": "^5.0.0",
    "prettier": "^2.0.0",
    "prismjs": "^1.19.0",
    "react-syntax-highlighter": "^13.0.0",
    "styled-components": "^5.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/khriztianmoreno/storybook-addon-raw-styled-component/issues"
  },
  "homepage": "https://github.com/khriztianmoreno/storybook-addon-raw-styled-component/",
  "directories": {
    "example": "examples",
    "lib": "lib"
  }
}
